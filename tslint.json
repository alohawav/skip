{
  "extends": ["tslint-config-airbnb", "tslint-config-prettier", "tslint-react"],
  "rules": {
    "semicolon": false,
    "max-line-length": [true, 100],
    "import-name": false,
    "variable-name": false,
    "jsx-no-multiline-js": false,
    "jsx-boolean-value": false,
    "jsx-no-lambda": false
  }
}
